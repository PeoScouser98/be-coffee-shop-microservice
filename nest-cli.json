{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/auth/src",
  "root": "apps/auth",
  "monorepo": true,
  "compilerOptions": {
    "tsConfigPath": "apps/auth/tsconfig.app.json",
    "deleteOutDir": true,
    "webpack": true,
    "assets": [
      {
        "include": "i18n/**/*",
        "watchAssets": true,
        "outDir": "dist/apps/api"
      }
    ]
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json",
        "deleteOutDir": true,
        "webpack": true
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "i18n": {
      "type": "library",
      "root": "libs/i18n",
      "entryFile": "index",
      "sourceRoot": "libs/i18n/src",
      "compilerOptions": {
        "tsConfigPath": "libs/i18n/tsconfig.lib.json"
      }
    },
    "rmq": {
      "type": "library",
      "root": "libs/rmq",
      "entryFile": "index",
      "sourceRoot": "libs/rmq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rmq/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json",
        "webpack": true,
        "deleteOutDir": true
      }
    },
    "product": {
      "type": "application",
      "root": "apps/product",
      "entryFile": "main",
      "sourceRoot": "apps/product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product/tsconfig.app.json",
        "webpack": true,
        "deleteOutDir": true
      }
    },
    "discount": {
      "type": "application",
      "root": "apps/discount",
      "entryFile": "main",
      "sourceRoot": "apps/discount/src",
      "compilerOptions": {
        "tsConfigPath": "apps/discount/tsconfig.app.json",
        "builder": "webpack",
        "deleteOutDir": true
      }
    },
    "retail-store": {
      "type": "application",
      "root": "apps/retail-store",
      "entryFile": "main",
      "sourceRoot": "apps/retail-store/src",
      "compilerOptions": {
        "tsConfigPath": "apps/retail-store/tsconfig.app.json",
        "builder": "webpack",
        "deleteOutDir": true
      }
    },
    "inventory": {
      "type": "application",
      "root": "apps/inventory",
      "entryFile": "main",
      "sourceRoot": "apps/inventory/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory/tsconfig.app.json",
        "builder": "webpack",
        "deleteOutDir": true
      }
    },
    "purchase-order": {
      "type": "application",
      "root": "apps/purchase-order",
      "entryFile": "main",
      "sourceRoot": "apps/purchase-order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/purchase-order/tsconfig.app.json",
        "builder": "webpack",
        "deleteOutDir": true
      }
    },
    "shopping-cart": {
      "type": "application",
      "root": "apps/shopping-cart",
      "entryFile": "main",
      "sourceRoot": "apps/shopping-cart/src",
      "compilerOptions": {
        "tsConfigPath": "apps/shopping-cart/tsconfig.app.json",
        "webpack": true
      }
    },
    "post": {
      "type": "application",
      "root": "apps/post",
      "entryFile": "main",
      "sourceRoot": "apps/post/src",
      "compilerOptions": {
        "tsConfigPath": "apps/post/tsconfig.app.json"
      }
    }
  }
}